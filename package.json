{
  "name": "@rentspree/path",
  "version": "0.1.16",
  "description": "URL path params/query resolve with ease",
  "license": "MIT",
  "repository": "rentspree/path",
  "main": "dist/index.js",
  "author": {
    "name": "Potsawee Vechpanich",
    "email": "potsawee@rentspree.com",
    "url": "https://github.com/rentspree"
  },
  "engines": {
    "node": ">=7"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "mocha --require babel-register",
    "coverage": "npm test -- --coverage",
    "postcoverage": "opn coverage/lcov-report/index.html",
    "lint": "eslint src test",
    "flow": "flow check",
    "docs": "documentation readme src --section=API",
    "clean": "rimraf dist",
    "flowbuild": "flow-copy-source src dist",
    "prebuild": "npm run docs && npm run clean && npm run flowbuild",
    "build": "babel src -d dist",
    "watch": "npm-watch",
    "xprepublishOnly": "npm run lint && npm test && npm run build",
    "xpostpublish": "git push origin master --follow-tags"
  },
  "watch": {
    "test": "{src,test}/*.js",
    "lint": "{src,test}/*.js",
    "build": "src"
  },
  "keywords": [
    "rentspree-path"
  ],
  "dependencies": {
    "lodash": "^4.17.11",
    "path-to-regexp": "^2.2.1",
    "qs": "^6.5.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "documentation": "^9.3.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-flowtype-errors": "^3.5.1",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-mocha": "^5.0.0",
    "eslint-plugin-prettier": "^2.6.0",
    "flow-bin": "^0.73.0",
    "flow-copy-source": "^1.3.0",
    "mocha": "^5.2.0",
    "npm-watch": "^0.6.0",
    "opn-cli": "^3.1.0",
    "prettier": "^1.13.7",
    "rimraf": "^2.6.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
