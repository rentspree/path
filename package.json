{
  "name": "@rentspree/path",
  "version": "0.2.1",
  "description": "URL path params/query resolve with ease",
  "license": "MIT",
  "repository": "rentspree/path",
  "main": "dist/index.js",
  "author": {
    "name": "Potsawee Vechpanich",
    "email": "potsawee@rentspree.com",
    "url": "https://github.com/rentspree"
  },
  "engines": {
    "node": ">=7"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "mocha --require @babel/register",
    "test:watch": "mocha --require @babel/register --watch",
    "coverage": "npm test -- --coverage",
    "postcoverage": "opn coverage/lcov-report/index.html",
    "lint": "eslint src test",
    "flow": "flow check",
    "docs": "documentation readme src --section=API",
    "clean": "rimraf dist",
    "flowbuild": "flow-copy-source src dist",
    "prebuild": "npm run docs && npm run clean && npm run flowbuild",
    "build": "babel src -d dist",
    "watch": "npm-watch",
    "xprepublishOnly": "npm run lint && npm test && npm run build",
    "xpostpublish": "git push origin master --follow-tags"
  },
  "watch": {
    "test": "{src,test}/*.js",
    "lint": "{src,test}/*.js",
    "build": "src"
  },
  "keywords": [
    "rentspree-path"
  ],
  "dependencies": {
    "lodash": "^4.17.11",
    "path-to-regexp": "^2.2.1",
    "qs": "^6.5.2",
    "url-join": "^4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.2.2",
    "@babel/preset-env": "7.2.3",
    "@babel/preset-flow": "^7.0.0",
    "@babel/register": "^7.4.4",
    "babel-eslint": "^9.0.0",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "chai": "^4.1.2",
    "documentation": "^11.0.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^5.0.0",
    "eslint-plugin-flowtype": "^3.10.3",
    "eslint-plugin-flowtype-errors": "^4.1.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-mocha": "^5.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "flow-bin": "^0.101.1",
    "flow-copy-source": "^1.3.0",
    "mocha": "^5.2.0",
    "mongoose": "^5.6.0",
    "npm-watch": "^0.6.0",
    "opn-cli": "^3.1.0",
    "prettier": "1.18.2",
    "rimraf": "^2.6.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
